{
  "dataSource": "prs",
  "groupBy": {
    "Enhancements:": ["enhancement", "internal"],
    "Bug Fixes:": ["bug"]
  },
  "template": {
    "commit": "- [{{message}}]({{url}}) - @{{author}}",
    "issue": "- {{name}} [{{text}}]({{url}})",
    "label": "[**{{label}}**]",
    "noLabel": "closed",
    "group": "\n#### {{heading}}\n",
    "changelogTitle": "# Changelog\n\n",
    "release": "## {{release}} ({{date}})\n{{body}}",
    "releaseSeparator": "\n---\n\n"
  }
}